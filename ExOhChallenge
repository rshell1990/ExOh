function StringChallenge(str) { 

  if (str.length %2 === 1){
    return false;
  }
  else {
    var exOh = 0;
    for (var i = 0; i < str.length; i++){
      if(str[i] === "x"){
      exOh++
      }
    }
    if(exOh === (str.length/2)){
      return true;
    }
    else{
      return false;
    }
  }

}
   
// keep this function call here 
console.log(StringChallenge(readline()));
